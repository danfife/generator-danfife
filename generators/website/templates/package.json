{
  "name": "<%= name %>",
  "version": "0.0.0",
  "description": "",
  "main": "",
  "scripts": {
    "test": "karma start --single-run",
    "watch:test": "karma start",
    "assets": "node scripts/assets.js -s -f \"**/*.{css,js} images/**/*.*\" -r \"**/*.{css,html}\"",
    "clean": "rimraf dist/* dist .tmp/* .tmp -s",
    "build": "npm run clean -s && npm run dev:build -s && npm run prod:build -s",
    "dev": "npm run dev:build -s && run-p -s dev:open dev:server dev:watch:build dev:livereload watch:test",
    "dev:build": "npm run dev:build:assets -s && rimraf dist/dev/* -s && copyup .tmp/dev/* .tmp/dev/**/* dist && rimraf .tmp/dev/*",
    "dev:build:assets": "rimraf .tmp/dev/* -s && npm run dev:build:static -s && npm run dev:build:js -s && npm run dev:build:css -s && npm run assets -s -- -p .tmp/dev",
    "dev:build:js": "browserify -d public/js/main.js > .tmp/dev/main.js",
    "dev:build:css": "node-sass --output-style expanded public/scss/main.scss > .tmp/dev/main.css",
    "dev:build:static": "copyup public/**/*.{html,png,jpg} .tmp/dev",
    "dev:server": "http-server -s -p 8080 dist/dev",
    "dev:open": "opener http://localhost:8080/index.html",
    "dev:watch:build": "nodemon -q -w public/ --ext \".\" --exec \"npm run dev:build\"",
    "dev:livereload": "livereload dist/dev -w 200",
    "prod:build": "rimraf dist/prod/* -s && npm run prod:build:assets -s && npm run assets -s -- -p dist/prod -u \"https://cdn2.com\"",
    "prod:build:assets": "npm run prod:build:static -s && npm run prod:build:js -s && npm run prod:build:css -s",
    "prod:build:js": "browserify -t uglifyify -o dist/prod/main.js ./public/js/main.js",
    "prod:build:css": "node-sass --output-style compact public/scss/main.scss > dist/prod/main.css",
    "prod:build:static": "copyup public/**/*.{html,png,jpg} dist/prod"
  },
  "keywords": [],
  "author": "daniel.fife@gmail.com",
  "license": "MIT",
  "dependencies": {
  },
  "devDependencies": {
    "argparse": "^1.0.9",
    "browserify": "^15.2.0",
    "chai": "^4.1.2",
    "copyfiles": "^1.2.0",
    "karma": "^2.0.0",
    "karma-browserify": "^5.1.3",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jquery": "^0.2.2",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sinon": "^1.0.5",
    "livereload": "^0.6.3",
    "lodash": "^4.17.4",
    "minifyify": "^7.3.5",
    "mocha": "^5.0.0",
    "node-sass": "^4.7.2",
    "node-version-assets": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "opener": "^1.4.3",
    "rimraf": "^2.6.2",
    "watchify": "^3.10.0"
  }
}
