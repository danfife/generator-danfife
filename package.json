{
  "name": "generator-danfife",
  "version": "1.1.0",
  "description": "danfife generators",
  "homepage": "",
  "author": {
    "name": "danfife",
    "email": "daniel.fife@gmail.com",
    "url": "https://github.com/danfife/generator-danfife"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "generator-danfife",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "nsp": "^2.6.3",
    "eslint": "^4.1.0",
    "prettier": "^1.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-config-xo": "^0.18.2",
    "jest": "^22.0.6"
  },
  "dependencies": {
    "yeoman-generator": "^2.0.1",
    "chalk": "^2.1.0",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/templates/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/templates/"
    ]
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "eslint:fix": "eslint . --fix",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danfife/generator-danfife.git"
  },
  "license": "MIT"
}
